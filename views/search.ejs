<%- include("parts/header") %>
<body class="w-100 ">
  <div class="container my-5 py-5">
    <form class=" py-4 d-flex justify-content-center">
      <input class="form-control w-75" list="datalistOptions" id="exampleDataList" placeholder="Escolha uma competÃªncia...">
      <datalist id="datalistOptions">
        <% competencias.forEach(competencia => { %>
          <option value=<%= competencia %>>
        <% }) %>
      </datalist>
    </form>

    <section class="cards">
      <% tutores.forEach(tutor => { %>
        <div class="card text-center" style="width: 18rem;">
          <div class="card-body">
            <h5 class="card-title"><%= tutor.salasCriadas[0].nome  %></h5>
            <p class="card-text">Tutor: <%= tutor.nome  %></p>          
            <p class="card-text">Encontros as sextas.</p>
            <a href="/class" class="btn btn-primary">Entrar</a>
          </div>
        </div>
      <% }) %>
    </section>
  </div>
</body>
</html>